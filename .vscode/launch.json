    "version": "0.2.0",
    "configurations": [
        {
            "name": "PRUEBA.AUTH.API (HTTPS)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/PRUEBA.AUTH.API/bin/Debug/net8.0/PRUEBA.AUTH.API.dll",
            "args": [],
            "cwd": "${workspaceFolder}/PRUEBA.AUTH.API",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)",
                "uriFormat": "%s/swagger"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "https://localhost:7208;http://localhost:5228"
            }
        },
